using System;
using System.Collections.Generic;
using System.ComponentModel;
using System.Data;
using System.Drawing;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Windows.Forms;

namespace BankPraca
{
    public partial class Wybor : Form
    {
        public Wybor()
        {
            InitializeComponent();
        }

        private void Nowyklient_Click(object sender, EventArgs e)
        {
            this.Hide();
            Nowyklient nowy = new BankPraca.Nowyklient();
            nowy.Show();
        }

        private void Stalyklient_Click(object sender, EventArgs e)
        {
            this.Hide();
            StalyLogowanie staly = new StalyLogowanie();
            staly.Show();
        }
    }
}
